app = angular.module("Manticore", [
    'ui.bootstrap.dialog',
    'ui.bootstrap.collapse',
    'resources.countries'
])
.config(["$httpProvider", "$anchorScrollProvider", ($httpProvider, $anchorScrollProvider)->
        $httpProvider.defaults.headers.common['X-CSRF-Token'] = $('meta[name=csrf-token]').attr('content')
    ])
.run(["$rootScope", "$http",  ($rootScope, $http) ->
        $rootScope.$debugMode = "on"
    ])

